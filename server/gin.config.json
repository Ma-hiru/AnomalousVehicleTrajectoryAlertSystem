{
  "port": 824,
  "mode": "debug",
  "stack": true,
  "ssl": false,
  "key": "./key.pem",
  "chain": "./chain.pem",
  "cors": {
    "allow_methods": [
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "DELETE",
      "OPTIONS",
      "HEAD"
    ],
    "allow_headers": [
      "Origin",
      "Content-Length",
      "Content-Type",
      "Authorization"
    ],
    "allow_credentials": true,
    "allow_all_origins": true,
    "allow_origins": [],
    "expose_headers": [
      "Content-Length"
    ],
    "max_age": 86400000000000,
    "allow_web_sockets": true,
    "allow_files": true
  }
}